# In các số chẵn từ 10 đến 50
print("Các số chẵn từ 10 đến 50:")
for i in range(10, 51):
    if i % 2 == 0:
        print(i, end=" ")

print()  # Xuống dòng

# Tính tổng các số từ 10 đến 50
tong = 0
for i in range(10, 51):
    tong += i
print("Tổng các số từ 10 đến 50 là:", tong)



quại m
n = int(input("Nhập n: "))
tich = 1
for i in range(1, n + 1):
    tich *= i
print(f"Tích từ 1 đến {n} là: {tich}")
