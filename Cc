n = int(input("Nhập số phần tử của danh sách: "))
ds = []
for i in range(n):
    x = int(input(f"Nhập phần tử thứ {i+1}: "))
    ds.append(x)
print("Danh sách vừa nhập là:", ds)
tong = 0
dem = 0
for x in ds:
    if x > 0:
        tong += x
        dem += 1
if dem > 0:
    tbc = tong / dem
    print("Trung bình cộng các số nguyên dương là:", tbc)
else:
    print("Không có số nguyên dương nào trong danh sách.")
k = int(input("Nhập số k cần đếm: "))
dem_k = ds.count(k)
print(f"Số {k} xuất hiện {dem_k} lần trong danh sách.")
